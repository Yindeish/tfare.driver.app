{
  "expo": {
    "name": "tfare",
    "slug": "tfare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EF5DA8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yindeish.tfare"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.yindeish.tfare"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-image-picker",
      "expo-location"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6210a570-fa7b-4b2f-9985-5545579b0c9a"
      }
    }
  }
}
